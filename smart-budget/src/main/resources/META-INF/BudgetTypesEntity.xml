<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.BudgetTypesEntity">
        <table name="budget_types" schema="public" catalog="budget"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="createdAt">
                <column name="created_at" precision="-1"/>
            </basic>
            <one-to-many name="budgetsesById" mapped-by="budgetTypesByTypeId"
                         target-entity="entity.BudgetsEntity"/>
            <one-to-many name="recurringsesById" mapped-by="budgetTypesByBudgetTypeId"
                         target-entity="entity.RecurringsEntity"/>
        </attributes>
    </entity>
</entity-mappings>
