<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.BudgetsEntity">
        <table name="budgets" schema="public" catalog="budget"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" length="256"/>
            </basic>
            <basic name="projected">
                <column name="projected" precision="2"/>
            </basic>
            <basic name="actual">
                <column name="actual" precision="2"/>
            </basic>
            <basic name="periodOn">
                <column name="period_on" precision="-1"/>
            </basic>
            <basic name="createdAt">
                <column name="created_at" precision="-1"/>
            </basic>
            <many-to-one name="usersByUserId" optional="true" target-entity="entity.UsersEntity">
                <join-column name="user_id" referenced-column-name="id"/>
            </many-to-one>
            <many-to-one name="categoriesByCategoryId" optional="true"
                         target-entity="entity.CategoriesEntity">
                <join-column name="category_id" referenced-column-name="id"/>
            </many-to-one>
            <many-to-one name="budgetTypesByTypeId" optional="true"
                         target-entity="entity.BudgetTypesEntity">
                <join-column name="type_id" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="transactionsesById" mapped-by="budgetsByBudgetId"
                         target-entity="entity.TransactionsEntity"/>
        </attributes>
    </entity>
</entity-mappings>
