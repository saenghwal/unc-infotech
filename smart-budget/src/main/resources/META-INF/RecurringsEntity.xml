<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.RecurringsEntity">
        <table name="recurrings" schema="public" catalog="budget"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="amount">
                <column name="amount" precision="2"/>
            </basic>
            <basic name="type">
                <column name="type" length="256"/>
            </basic>
            <basic name="lastRunAt">
                <column name="last_run_at" precision="-1"/>
            </basic>
            <basic name="createdAt">
                <column name="created_at" precision="-1"/>
            </basic>
            <basic name="remark">
                <column name="remark" nullable="false"/>
            </basic>
            <many-to-one name="budgetTypesByBudgetTypeId" optional="true"
                         target-entity="entity.BudgetTypesEntity">
                <join-column name="budget_type_id" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="transactionsesById" mapped-by="recurringsByRecurringId"
                         target-entity="entity.TransactionsEntity"/>
        </attributes>
    </entity>
</entity-mappings>
